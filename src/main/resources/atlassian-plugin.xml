<?xml version="1.0" encoding="UTF-8"?>

<atlassian-plugin key="${atlassian.plugin.key}" name="${project.name}" plugins-version="2"> 
  <plugin-info> 
    <description>${project.description}</description>  
    <version>${project.version}</version>  
    <vendor name="${project.organization.name}" url="${project.organization.url}"/>  
    <param name="plugin-icon">images/pluginIcon.png</param>  
    <param name="plugin-logo">images/pluginLogo.png</param> 
  </plugin-info>  
  <!-- add our i18n resource -->  
  <resource type="i18n" name="i18n" location="spring-plugin"/>  
  <!-- add our web resources -->  
  <web-resource key="spring-plugin-resources" name="spring-plugin Web Resources"> 
    <dependency>com.atlassian.auiplugin:ajs</dependency>  
    <resource type="download" name="spring-plugin.css" location="/css/spring-plugin.css"/>
    <resource type="download" name="spring-plugin.js" location="/js/spring-plugin.js"/>
    <resource type="download" name="images/" location="/images"/>
    <context>spring-plugin</context>
  </web-resource>
  <web-section name="I wanna Die" i18n-name-key="my-links-main-section.name" key="my_links_section" location="my_link_link" weight="10"/>
  <web-item key="my_links_link" name="My vk" section="jira.project.sidebar.plugins.navigation" weight="47">
    <label>Export</label>
    <link linkId="my_links_link">/secure/ExcelPluginCreateAction!default.jspa?projectKey=${project.key}</link>
  </web-item>
  <webwork1 key="excel-plugin-create" name="ExcelPluginCreate" i18n-name-key="excel-plugin-create.name">
    <description key="excel-plugin-create.description">The ExcelPluginCreate Plugin</description>
    <actions>
      <action name="ru.shevtsov.ilya.plugins.spring.jira.webwork.ExcelPluginCreateAction" alias="ExcelPluginCreateAction">
        <view name="success">/projects</view>
      </action>
    </actions>
  </webwork1>
  <jql-function name="Recent Project Function" i18n-name-key="recent-project-function.name" key="recent-project-function"
                class="ru.shevtsov.ilya.plugins.spring.jira.jql.RecentProjectFunction">
    <description key="recent-project-function.description">The Recent Project Function Plugin</description>
    <fname>recentProjects</fname>
    <list>true</list>
  </jql-function>
</atlassian-plugin>
